{
  "name": "pt-tools-browser-extension",
  "version": "0.1.0",
  "private": true,
  "description": "PT Tools Helper browser extension",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "pack": "node --experimental-strip-types ../../scripts/check-sites.ts && vite build && cd dist && zip -r ../pt-tools-helper.zip .",
    "typecheck": "vue-tsc --noEmit",
    "clean": "rm -rf dist pt-tools-helper.zip"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.31",
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vue-tsc": "^3.1.1"
  },
  "packageManager": "pnpm@10.25.0"
}
